{
  "name": "f-icon-poc",
  "version": "1.0.15",
  "description": "icon poc",
  "module": "dist/flow-core.es.js",
  "types": "dist/types/src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "./compile.sh",
    "build": "vite build --emptyOutDir && tsc -emitDeclarationOnly",
    "build:watch": "tsc --watch",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "storybook": "start-storybook -p 6006",
    "start": "yarn storybook",
    "build-storybook": "build-storybook",
    "test": "yarn build && web-test-runner ./src/**/*.test.ts --node-resolve",
    "test:watch": "yarn build && web-test-runner ./src/**/*.test.ts --node-resolve --watch"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "dependencies": {
    "f-core-poc": "^1.0.3",
    "lit": "^2.2.4"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@open-wc/testing": "^3.1.5",
    "@storybook/addon-actions": "^6.5.7",
    "@storybook/addon-essentials": "^6.5.7",
    "@storybook/addon-links": "^6.5.7",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/web-components": "^6.5.7",
    "@types/eslint": "^8.4.3",
    "@types/jest": "28.1.3",
    "@types/prettier": "^2.6.3",
    "@web/dev-server-esbuild": "^0.3.0",
    "@web/dev-server-rollup": "0.3.18",
    "@web/test-runner": "^0.13.30",
    "esbuild-sass-plugin": "2.2.6",
    "eslint": "^8.17.0",
    "lit-html": "^2.2.5",
    "prettier": "^2.6.2",
    "rollup-plugin-postcss-lit": "^2.0.0",
    "rollup-plugin-scss": "3.0.0",
    "sass": "^1.52.3",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vue": "2.6.14"
  },
  "peerDependencies": {
    "f-core-poc": "^1.0.3"
  },
  "customElements": "custom-elements.json"
}
